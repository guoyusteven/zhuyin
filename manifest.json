{

	"manifest_version": 2,
	
	"name": "Zhuyin",
	"description": "Add zhuyin fuhao next to Chinese characters, port of Phoebe Tai's Chrome ext.",
	"version": "1.0",

	"background": {
		"scripts": ["zhuyin.js"],
		"persistent": false
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Zhuyin"
	},

	"permissions": [
		"activeTab"
	],

	"web_accessible_resources": [
		"wp010-05.ttf"
	]

}
