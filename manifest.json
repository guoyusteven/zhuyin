{

	"manifest_version": 2,
	"name": "Zhuyin",
	"description": "Adds zhuyin fuhao next to Chinese characters.",
	"version": "2.0",

	"background": {
		"scripts": ["zhuyin.js"],
		"persistent": false
	},

	"browser_action": {
		"default_title": "Zhuyin"
	},

	"permissions": [
		"activeTab"
	],

	"web_accessible_resources": [
		"wp010-08.ttf"
	]

}
